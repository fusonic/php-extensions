{
    "name": "fusonic/api-documentation-bundle",
    "license": "MIT",
    "version": "0.0.7",
    "description": "Symfony bundle for automated documentation with NelmioApiDocBundle.",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Fusonic GmbH",
            "email": "office@fusonic.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fusonic\\ApiDocumentationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fusonic\\ApiDocumentationBundle\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/App/TestKernel.php"
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.13",
        "symfony/framework-bundle": "^5.4.14|^6.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-symfony": "^1.1",
        "symfony/test-pack": "^1.0",
        "symfony/yaml": "^5.4.14|^6.0"
    },
    "require": {
        "php": ">=8.1",
        "symfony/config": "^5.4.14|^6.0",
        "symfony/dependency-injection": "^5.4.14|^6.0",
        "symfony/routing": "^5.4.14|^6.0",
        "nelmio/api-doc-bundle": "^4.9",
        "symfony/property-info": "^5.4.14|^6.0",
        "phpunit/phpunit": "^9.5",
        "zircote/swagger-php": "^4.4",
        "symfony/dom-crawler": "^5.4.14|^6.0"
    },
    "scripts": {
        "phpstan": "XDEBUG_MODE=off php -d memory_limit=2048M vendor/bin/phpstan analyse",
        "phpcs-check": "XDEBUG_MODE=off vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php -v --dry-run --diff --using-cache=yes --allow-risky=yes",
        "phpcs-fix": "XDEBUG_MODE=off vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php -v --using-cache=yes --allow-risky=yes",
        "test": "XDEBUG_MODE=off vendor/bin/phpunit --testdox",
        "test-coverage" : "XDEBUG_MODE=off vendor/bin/phpunit --coverage-text --coverage-cobertura=coverage/cobertura.xml --coverage-html=coverage/html"
    }
}
